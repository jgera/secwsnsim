<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
<simulation>
<settings>
<name>Console Simulation</name>
<seed>1024</seed>
<fastmode>true</fastmode>
<nodeFactoryClassName>org.wisenet.simulator.temp.insens.INSENSNodeFactory</nodeFactoryClassName>
<simulatorClassName>org.wisenet.simulator.core.Simulator</simulatorClassName>
<radioModelClassName>org.wisenet.simulator.core.radio.GaussianRadioModel</radioModelClassName>
<maxNodeRadioRange>130</maxNodeRadioRange>
<energyModelClassName>org.wisenet.simulator.core.energy.EnergyModel</energyModelClassName>
<staticz>true</staticz>
<minz>0.0</minz>
<maxz>0.0</maxz>
<environAttenuation>0.0</environAttenuation>
</settings>
</simulation>
<MACLayer>
<maxAllowedNoiseOnSending>
<label>maxAllowedNoiseOnSending</label>
<value>5.0</value>
<required>true</required>
</maxAllowedNoiseOnSending>
<sendRandomWaitingTime>
<label>sendRandomWaitingTime</label>
<value>128</value>
<required>true</required>
</sendRandomWaitingTime>
<sendMinBackOffTime>
<label>sendMinBackOffTime</label>
<value>100</value>
<required>true</required>
</sendMinBackOffTime>
<receivingStartSNR>
<label>receivingStartSNR</label>
<value>5.0</value>
<required>true</required>
</receivingStartSNR>
<sendMinWaitingTime>
<label>sendMinWaitingTime</label>
<value>200</value>
<required>true</required>
</sendMinWaitingTime>
<sendRandomBackOffTime>
<label>sendRandomBackOffTime</label>
<value>30</value>
<required>true</required>
</sendRandomBackOffTime>
<sendTransmissionTime>
<label>sendTransmissionTime</label>
<value>960</value>
<required>true</required>
</sendTransmissionTime>
<noiseVariance>
<label>noiseVariance</label>
<value>0.025</value>
<required>true</required>
</noiseVariance>
<corruptionSNR>
<label>corruptionSNR</label>
<value>2.0</value>
<required>true</required>
</corruptionSNR>
</MACLayer>
</configuration>
